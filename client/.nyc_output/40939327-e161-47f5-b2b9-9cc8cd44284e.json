e;\r\n    case 'DELETE':\r\n      console.log('DELETE');\r\n\r\n      return {\r\n        ...state,\r\n        items: [\r\n          ...state.items.filter(todo =>\r\n            todo._id !== action.payload._id ? { ...todo } : null,\r\n          ),\r\n        ],\r\n      };\r\n    default:\r\n      return state;\r\n  }\r\n};\r\n\r\nfunction ActionProvider({ children }) {\r\n  const [state, dispatch] = React.useReducer(ActionReducer, {\r\n    items: items,\r\n  });\r\n\r\n  return (\r\n    <>\r\n      <DispatchContext.Provider value={dispatch}>\r\n        <StateContext.Provider value={state}>{children}</StateContext.Provider>\r\n      </DispatchContext.Provider>\r\n    </>\r\n  );\r\n}\r\nexport { ActionProvider, StateContext, DispatchContext, ActionReducer };\r\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"35f75dbcd65e46224c5a7e2296eb45617ef1fac7","contentHash":"5a3c0ffd373e52ac55246ea3924fca1bd67b84fd1dc662797d7b43b1a414d951"}}