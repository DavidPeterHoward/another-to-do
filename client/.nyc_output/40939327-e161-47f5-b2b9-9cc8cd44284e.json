onst { checked, changeCompleted } = useChangeIsChecked(\r\n    props.completed,\r\n    props.id,\r\n  );\r\n  return (\r\n    <>\r\n      <Todo completed={checked} onClick={changeCompleted}>\r\n        {props.children}\r\n      </Todo>\r\n      <Checkbox checked={checked} onChange={changeCompleted} />\r\n      <DeleteTodo id={props.id} />\r\n    </>\r\n  );\r\n};\r\n\r\nconst DeleteTodo = props => {\r\n  const { HandleDeleteTodo } = useDelete(props.id);\r\n\r\n  return <div onClick={HandleDeleteTodo}>X</div>;\r\n};\r\n\r\nconst MemoizeTodoComponent = React.memo(TodoComponent);\r\n\r\nexport default MemoizeTodoComponent;\r\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"29b1868ab9bc23c171ed01867366ff24ccf6a74c","contentHash":"46ce583b88ae861feaf08c0e5079d81c28aac5881e634263891fc247a4a2c066"},"C:\\_2020\\2. FEB20\\2. React Testing Practice\\1. Testing Practice\\client\\src\\contexts\\Context.js":{"path":"C:\\_2020\\2. FEB20\\2. React Testing Practice\\1. Testing Practice\\client\\src\\contexts\\Context.js","statementMap":{"0":{"start":{"line":3,"column":0},"end":{"line":5,"column":3}},"1":{"start":{"line":6,"column":0},"end":{"line":6,"column":40}},"2":{"start":{"line":7,"column":0},"end":{"line":7,"column":80}},"3":{"start":{"line":9,"column":13},"end":{"line":9,"column":53}},"4":{"start":{"line":11,"column":39},"end":{"line":11,"column":93}},"5":{"start":{"line":13,"column":21},"end":{"line":13,"column":51}},"6":{"start":{"line":15,"column":0},"end":{"line":15,"column":36}},"7":{"start":{"line":17,"column":24},"end":{"line":17,"column":54}},"8":{"start":{"line":19,"column":0},"end":{"line":19,"column":42}},"9":{"start":{"line":21,"column":9},"end":{"line":23,"column":1}},"10":{"start":{"line":22,"column":2},"end":{"line":22,"column":49}},"11":{"start":{"line":25,"column":14},"end":{"line":45,"column":2}},"12":{"start":{"line":46,"column":8},"end":{"line":46,"column":9}},"13":{"start":{"line":48,"column":22},"end":{"line":82,"column":1}},"14