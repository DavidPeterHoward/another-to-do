{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "react-router-dom": "^5.1.2",
    "react-hot-loader": "^4.12.19",
    "react-test-renderer": "^16.12.0",
    "styled-components": "^5.0.1",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.2.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@hot-loader/react-dom": "^16.11.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:unit": "mocha --require @babel/register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles 'src/**/*.spec.js'",
    "test:unit:watch": "npm run test:unit -- --watch",
    "nyc:unit": "nyc npm rum test:unit --reporter=text",
    "nyc:coverage": "nodemon --exec nyc npm rum test:unit --reporter=text",
    "coverage": "nyc mocha"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.styled.js"
    ]
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "proxy": "http://localhost:4000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
